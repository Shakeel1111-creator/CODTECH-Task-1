num1=int(input('enter the first number:'))
num2=int(input('enter the second number:'))
ch=int(input('1.Addition\n 2.Subtraction\n 3.Multiplication\n 4.Division\n enter your choice:'))
if(ch==1):
    print('Result= ',num1+num2)
elif (ch==2):
    print('Result= ',num1-num2)
elif (ch==3):
    print('Result= ',num1*num2)
elif (ch==4):
    print('Result= ',num1/num2)
else:
    print('Invalid Input')
